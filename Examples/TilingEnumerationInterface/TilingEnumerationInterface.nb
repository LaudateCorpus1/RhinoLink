            "}"}]}]}], ")"}], "&"}], " ", "/@", " ", "cells"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Canonicalize", " ", "for", " ", "cyclic", " ", "rotations", " ", "of", 
       " ", "the", " ", "unit", " ", "cell"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"cells", " ", "=", " ", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Flatten", "[", 
