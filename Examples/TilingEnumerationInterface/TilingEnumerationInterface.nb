       RowBox[{
        RowBox[{"TransformUnitCell", "[", 
         RowBox[{"cellIn", ",", "#"}], "]"}], "&"}], " ", "/@", " ", 
       RowBox[{"If", "[", 
        RowBox[{"isSquare", ",", 
         RowBox[{"SquareTransforms", "[", "]"}], ",", 
         RowBox[{"RectangleTransforms", "[", "]"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Canonicalize", " ", "unit", " ", "cell", " ", "transforms"}], 
