Cell[BoxData[
 RowBox[{
  RowBox[{"CanonicalizeUnitCell", "[", 
   RowBox[{"cellIn_", ",", " ", "labeledTiles_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"isSquare", ",", "cells", ",", " ", "cell"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"isSquare", " ", "=", " ", 
