  RowBox[{
   RowBox[{"RheotomicSurface", "[", 
    RowBox[{"pts_", ",", 
     RowBox[{"s_:", "1"}], ",", 
     RowBox[{"range_:", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
