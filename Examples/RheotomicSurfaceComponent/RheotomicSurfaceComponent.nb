     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"xMin", ",", "xMax"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"yMin", ",", "yMax"}], "}"}]}], "}"}], " ", "=", " ", 
