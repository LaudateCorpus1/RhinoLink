Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"RheotomicSurface", "[", 
    RowBox[{"pts_", ",", 
     RowBox[{"s_:", "1"}], ",", 
     RowBox[{"range_:", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "}"}]}], ",", 
     RowBox[{"n_:", "1"}], ",", " ", 
     RowBox[{"\[Delta]_:", ".1"}], ",", 
     RowBox[{"\[Epsilon]_:", "0.0001"}]}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "x", ",", "y", ",", "xMin", ",", "xMax", ",", "yMin", ",", "yMax", ",", 
       "yRanges", ",", "nurbs"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"xMin", ",", "xMax"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"yMin", ",", "yMax"}], "}"}]}], "}"}], " ", "=", " ", 
       "range"}], ";", "\[IndentingNewLine]", 
      RowBox[{"cuts", " ", "=", " ", 
       RowBox[{"Union", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "2"}], "]"}], "]"}], "&"}], " ", "/@", " ", 
         "pts"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"sum", "=", 
          RowBox[{"N", "[", 
           RowBox[{"Im", "[", 
            RowBox[{"Total", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], 
                RowBox[{"Log", "[", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"x", "-", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ")"}], "+", 
                  RowBox[{"\[ImaginaryI]", 
                   RowBox[{"(", 
                    RowBox[{"y", "-", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ")"}]}]}], 
                 "]"}]}], "&"}], " ", "/@", " ", "pts"}], "]"}], "]"}], 
           "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"surf", "[", 
           RowBox[{"x_", ",", "y_"}], "]"}], ":=", "sum"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"yRanges", " ", "=", " ", 
          RowBox[{"Partition", "[", 
           RowBox[{
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", "yMin", "}"}], ",", "cuts", ",", 
              RowBox[{"{", "yMax", "}"}]}], "]"}], ",", "2", ",", "1"}], 
           "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"nurbs", " ", "=", " ", 
          RowBox[{
           RowBox[{
            RowBox[{"Module", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"y0", "=", 
                 RowBox[{"#", "[", 
                  RowBox[{"[", "1", "]"}], "]"}]}], ",", 
                RowBox[{"y1", "=", 
                 RowBox[{"#", "[", 
                  RowBox[{"[", "2", "]"}], "]"}]}], ",", "nx", ",", "ny"}], 
               "}"}], ",", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"nx", "=", 
                RowBox[{"Round", "[", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"xMax", "-", "xMin"}], ")"}], "/", "\[Delta]"}], 
                 "]"}]}], ";", "\[IndentingNewLine]", 
               RowBox[{"ny", "=", 
                RowBox[{"Round", "[", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"y1", "-", "y0"}], ")"}], "/", "\[Delta]"}], 
                 "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
               RowBox[{
               "NurbsSurface`CreateThroughPoints", "[", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"MakeNETObject", "[", 
                  RowBox[{
                   RowBox[{
                   "WolframScriptingPlugIn`ToRhinoPoint3dArray", "[", 
                    RowBox[{"Flatten", "[", 
                    RowBox[{
                    RowBox[{"Table", "[", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"x", ",", "y", ",", 
                    RowBox[{"s", " ", 
                    RowBox[{"surf", "[", 
                    RowBox[{"x", ",", "y"}], "]"}]}]}], "}"}], ",", 
                    "\[IndentingNewLine]", 
                    RowBox[{"{", 
                    RowBox[{"x", ",", 
                    RowBox[{"xMin", "+", "\[Epsilon]"}], ",", 
                    RowBox[{"xMax", "-", "\[Epsilon]"}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"xMax", "-", "xMin", "-", 
                    RowBox[{"2", "\[Epsilon]"}]}], ")"}], "/", "nx"}]}], 
                    "}"}], ",", "\[IndentingNewLine]", 
                    RowBox[{"{", 
                    RowBox[{"y", ",", 
                    RowBox[{"y0", "+", "\[Epsilon]"}], ",", 
                    RowBox[{"y1", "-", "\[Epsilon]"}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"y1", "-", "y0", "-", 
                    RowBox[{"2", "\[Epsilon]"}]}], ")"}], "/", "ny"}]}], 
                    "}"}]}], "]"}], ",", "1"}], "]"}], "]"}], ",", 
                   "\"\<Rhino.Geometry.Point3d[]\>\""}], "]"}], ",", 
                 "\[IndentingNewLine]", 
                 RowBox[{"nx", "+", "1"}], ",", 
                 RowBox[{"ny", "+", "1"}], ",", " ", "3", ",", " ", "3", ",", 
                 " ", "False", ",", "False"}], "\[IndentingNewLine]", 
                "]"}]}]}], "\[IndentingNewLine]", "]"}], "&"}], " ", "/@", 
           " ", "yRanges"}]}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", " ", 
          RowBox[{
           RowBox[{"duplicate", " ", "and", " ", "translate", " ", "n"}], "-", 
           RowBox[{"1", " ", "times"}]}], " ", "*)"}], "\[IndentingNewLine]", 
         
         RowBox[{"Join", "[", "\[IndentingNewLine]", 
          RowBox[{"nurbs", ",", "\[IndentingNewLine]", 
           RowBox[{"Flatten", "[", 
            RowBox[{"Table", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Module", "[", 
                 RowBox[{
                  RowBox[{"{", "geom", "}"}], ",", "\[IndentingNewLine]", 
                  RowBox[{
                   RowBox[{"geom", "=", 
                    RowBox[{"#", "@", 
                    RowBox[{"Duplicate", "[", "]"}]}]}], ";", 
                   "\[IndentingNewLine]", 
                   RowBox[{"geom", "@", 
                    RowBox[{"Translate", "[", 
                    RowBox[{"0", ",", "0", ",", 
                    RowBox[{"i", " ", "s", " ", "2", " ", "Pi"}]}], "]"}]}], 
                   ";", "\[IndentingNewLine]", "geom"}]}], 
                 "\[IndentingNewLine]", "]"}], "&"}], " ", "/@", " ", 
               "nurbs"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "1", ",", 
                RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], "]"}]}], 
          "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
