                RowBox[{"Log", "[", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"x", "-", 
                    RowBox[{"#", "[", 
                    RowBox[{"[", 
                    RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ")"}], "+", 
