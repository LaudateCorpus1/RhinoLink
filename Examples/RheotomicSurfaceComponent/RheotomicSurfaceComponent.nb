                    RowBox[{"Duplicate", "[", "]"}]}]}], ";", 
                   "\[IndentingNewLine]", 
                   RowBox[{"geom", "@", 
                    RowBox[{"Translate", "[", 
                    RowBox[{"0", ",", "0", ",", 
                    RowBox[{"i", " ", "s", " ", "2", " ", "Pi"}]}], "]"}]}], 
                   ";", "\[IndentingNewLine]", "geom"}]}], 
