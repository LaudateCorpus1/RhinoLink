                    RowBox[{"surf", "[", 
                    RowBox[{"x", ",", "y"}], "]"}]}]}], "}"}], ",", 
                    "\[IndentingNewLine]", 
                    RowBox[{"{", 
                    RowBox[{"x", ",", 
                    RowBox[{"xMin", "+", "\[Epsilon]"}], ",", 
                    RowBox[{"xMax", "-", "\[Epsilon]"}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"xMax", "-", "xMin", "-", 
                    RowBox[{"2", "\[Epsilon]"}]}], ")"}], "/", "nx"}]}], 
                    "}"}], ",", "\[IndentingNewLine]", 
                    RowBox[{"{", 
                    RowBox[{"y", ",", 
                    RowBox[{"y0", "+", "\[Epsilon]"}], ",", 
                    RowBox[{"y1", "-", "\[Epsilon]"}], ",", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"y1", "-", "y0", "-", 
                    RowBox[{"2", "\[Epsilon]"}]}], ")"}], "/", "ny"}]}], 
                    "}"}]}], "]"}], ",", "1"}], "]"}], "]"}], ",", 
